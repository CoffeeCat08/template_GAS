{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "..",
    "types": ["google-apps-script"],
    "baseUrl": ".",
    "paths": {
      "common/*": ["../common_typescript/*"]
    }
  },
  "include": ["src/**/*.ts", "../common_typescript/**/*.ts"],
  "exclude": ["src/functions/**", "src/types/**"]
}
